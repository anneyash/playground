<p id="notice"><%= notice %></p>

<h1>Visa Applications</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visa_applications.each do |visa_application| %>
      <tr>
        <td><%= visa_application.title %></td>
        <td><%= visa_application.body %></td>
        <td><%= link_to 'Show', visa_application %></td>
        <td><%= link_to 'Edit', edit_visa_application_path(visa_application) %></td>
        <td><%= link_to 'Destroy', visa_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visa Application', new_visa_application_path %>
